<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<commands>
    <command type="RTRV-VERSION">
        <arg>AID</arg>
        <keywords pos="2">
            <keyword>GISSUE</keyword>
            <keyword>GDBISSUE</keyword>
            <keyword>ACTDAT</keyword>
            <keyword>ACTTM</keyword>
            <keyword>VALTM</keyword>
            <keyword>DBSRST</keyword>
            <keyword>TYPE</keyword>
            <keyword>VERSION</keyword>
            <keyword>BLDDAT</keyword>
            <keyword>BLDTM</keyword>
        </keywords>
        <regex>(^.*)::(.*)</regex>
    </command>
    <command type="RTRV-COND-ALL">
        <arg>AID</arg>
        <arg>AIDTYPE</arg>
        <arg>NTFCNCDE </arg>
        <arg>CONDTYPE</arg>
        <arg>SRVEFF</arg>
        <arg>OCRDAT</arg>
        <arg>OCRTM</arg>
        <arg>LOCN</arg>
        <arg>DIRN</arg>
        <arg>TMPER</arg>
        <!--<arg>CONDDESCR</arg>-->
        <regex>(^.*),(.*):(.*),(.*),(.*),(.*),(.*),(.*),(.*?):(.*)</regex>
    </command>
    <command type="RTRV-EQPT">
        <arg>AID</arg>
        <arg>TYPE</arg>
        <arg>PST</arg>
        <arg optional="1">SST</arg>
        <keywords pos="3">
            <keyword>HKIN</keyword>
            <keyword>VENDID</keyword>
            <keyword>DOM</keyword>
            <keyword>CLEI</keyword>
            <keyword>SERIALNO</keyword>
            <keyword>UNITNAME</keyword>
            <keyword>USI</keyword>
            <keyword>LASER</keyword>
            <keyword>FREQMIN</keyword>
            <keyword>FREQMAX</keyword>
            <keyword>FREQGRID</keyword>
            <keyword>LMBDMIN</keyword>
            <keyword>LMBDMAX</keyword>
            <keyword>LMBDGRID</keyword>
            <keyword>LOCATION</keyword>
            <keyword>SHFTYPE</keyword>
            <keyword>BERSDFIC</keyword>
            <keyword>FICTYPE</keyword>
            <keyword>FFPCONF</keyword>
            <keyword>DCCMODE</keyword>
        </keywords>
        <regex>(^.*?):(.*?):(.*):(?:(.*),(.*)|(.*))</regex>
    </command>
    <command type="RTRV-FILE-NVM">
        <arg>AID</arg>
        <arg>BINTYPE</arg>
        <arg>PST</arg>
        <keywords pos="3">
            <keyword>BINISSUE</keyword>
            <keyword>UPDTDAT</keyword>
            <keyword>UPDTTM</keyword>
            <keyword>BKUPDAT</keyword>
            <keyword>BKUPTM</keyword>
            <keyword>COMMENT</keyword>
            <keyword>MEMLOC</keyword>
        </keywords>
        <regex>(^.*):(.*):(.*):(.*)</regex>
    </command>
    <command type="RTRV-FWINFO">
        <arg>AID</arg>
        <arg>PST</arg>
        <keywords pos="2">
            <keyword>BINISSUE</keyword>
            <keyword>EXPFWCOMPAT</keyword>
            <keyword>FWUGTYPE</keyword>
        </keywords>
        <regex>(^.*)::(.*):(.*)</regex>
    </command>
</commands>