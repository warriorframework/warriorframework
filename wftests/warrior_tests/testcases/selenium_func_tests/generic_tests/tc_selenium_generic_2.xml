<?xml version="1.0" ?>
<Testcase>
	<Details>
		<Name>tc_selenium_generic_2</Name>
		<Title>Test case to perform search action in google</Title>
		<ExpectedResults>PASS</ExpectedResults>
		<Category>search</Category>
		<Engineer>Sanika</Engineer>
		<Date/>
		<Time/>
		<default_onError action = 'next'  />
		<InputDataFile>../../../data/selenium_func_tests/data_single_sys.xml</InputDataFile>
		<Datatype>Custom</Datatype>
		<Logsdir/>
		<Resultsdir/>
	</Details>
	<Requirements>
		<Requirement>requirement-001</Requirement>
		<Requirement>requirement-002</Requirement>
	</Requirements>
	<Steps>
		<step TS= '1' Driver='selenium_driver' Keyword='browser_launch' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
			</Arguments>
		</step>
		<step TS= '2' Driver='selenium_driver' Keyword='get_element_by_xpath' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="xpath" value=".//*[@id='gbw']/div/div/div[1]/div[2]/a"/>
			</Arguments>
		</step>
		<step TS= '3' Driver='selenium_driver' Keyword='get_element_by_id' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="id" value="gbw"/>
			</Arguments>
		</step>
		<step TS= '4' Driver='selenium_driver' Keyword='get_element_by_link_text' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="link_text" value="Images"/>
			</Arguments>
		</step>
		<step TS= '5' Driver='selenium_driver' Keyword='get_element_by_tagname' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="tag_name" value="span"/>
			</Arguments>
		</step>
		<step TS= '6' Driver='selenium_driver' Keyword='get_element_by_classname' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="class_name" value="a4bIc"/>
			</Arguments>
		</step>
		<step TS= '7' Driver='selenium_driver' Keyword='get_element_by_name' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="name_of_element" value="btnI"/>
			</Arguments>
		</step>
		<step TS= '8' Driver='selenium_driver' Keyword='get_element' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="locator" value="btnI"/>
				<argument name="locator_type" value="name"/>
			</Arguments>
		</step>
		<step TS= '9' Driver='selenium_driver' Keyword='get_element' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
				<argument name="locator" value="span"/>
				<argument name="locator_type" value="tag"/>
			</Arguments>
		</step>
		<step TS= '10' Driver='selenium_driver' Keyword='browser_close' > <!-- Will launch or open a new browser in window -->
			<Arguments>
				<argument name="system_name" value="search_element"/>
			</Arguments>
		</step>
	</Steps>
</Testcase>
